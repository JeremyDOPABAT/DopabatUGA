@article{ ISI:000512473000002,
Author = {de Raynal, Paul-Eric Chaudru and Menozzi, Stephane and Priola, Enrico},
Title = {{Schauder estimates for drifted fractional operators in the supercritical
   case}},
Journal = {{JOURNAL OF FUNCTIONAL ANALYSIS}},
Year = {{2020}},
Volume = {{278}},
Number = {{8}},
Month = {{MAY 1}},
Abstract = {{We consider a non-local operator L-alpha which is the sum of a
   fractional Laplacian Delta(alpha/2), alpha is an element of (0,1), plus
   a first order term which is measurable in the time variable and locally
   beta-Holder continuous in the space variables. Importantly, the
   fractional Laplacian Delta(alpha/2) does not dominate the first order
   term. We show that global parabolic Schauder estimates hold even in this
   case under the natural condition alpha+beta > 1. Thus, the constant
   appearing in the Schauder estimates is in fact independent of the
   L-infinity-norm of the first order term. In our approach we do not use
   the so-called extension property and we can replace Delta(alpha/2) with
   other operators of alpha-stable type which are somehow close, including
   the relativistic alpha-stable operator. Moreover, when alpha is an
   element of (1/2, 1), we can prove Schauder estimates for more general
   alpha-stable type operators like the singular cylindrical one, i.e.,
   when Delta(alpha/2) is replaced by a sum of one dimensional fractional
   Laplacians Sigma(d)(k=1) (partial derivative(2)(xkxk))(alpha/2). (C)
   2019 Elsevier Inc. All rights reserved.}},
