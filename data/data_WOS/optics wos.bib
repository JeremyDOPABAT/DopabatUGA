@article{ ISI:000474317800012,
Author = {Hua, Yan-Fei and Guo, Bo-Ling and Ma, Wen-Xiu and Lu, Xing},
Title = {{Interaction behavior associated with a generalized (2+1)-dimensional
   Hirota bilinear equation for nonlinear waves}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
Year = {{2019}},
Volume = {{74}},
Pages = {{184-198}},
Month = {{OCT}},
Abstract = {{In this paper, we focus on the interaction behavior associated with a
   generalized (2+1)-dimensional Hirota bilinear equation. With symbolic
   computation, two types of interaction solutions including lump-kink and
   lump-soliton ones are derived through mixing two positive quadratic
   functions with an exponential function, or two positive quadratic
   functions with a hyperbolic cosine function in the bilinear equation.
   The completely non-elastic interaction between a lump and a stripe is
   presented, which shows the lump is drowned or shallowed by the stripe.
   The interaction between lump and soliton is also given, where the lump
   moves from one branch to the other branch of the soliton. These
   phenomena exhibit the dynamics of nonlinear waves and the solutions are
   useful for the study on interaction behavior of nonlinear waves in
   shallow water, plasma, nonlinear optics and Bose-Einstein condensates.
   (C) 2019 Elsevier Inc. All rights reserved.}},
