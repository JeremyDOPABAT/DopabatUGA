@article{ ISI:000469032700004,
Author = {de Mello, Daniel Ohl and Schaeffner, Dominik and Werkmann, Jan and
   Preuschoff, Tilman and Kohfahl, Lars and Schlosser, Malte and Birkl,
   Gerhard},
Title = {{Defect-Free Assembly of 2D Clusters of More Than 100 Single-Atom Quantum
   Systems}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2019}},
Volume = {{122}},
Number = {{20}},
Month = {{MAY 20}},
Abstract = {{We demonstrate the defect-free assembly of versatile target patterns of
   up 111 neutral atoms, building on a 361-site subset of a micro-optical
   architecture that readily provides thousands of sites for single-atom
   quantum systems. By performing multiple assembly cycles in rapid
   succession, we drastically increase achievable structure sizes and
   success probabilities. We implement repeated target pattern
   reconstruction after atom loss and deterministic transport of partial
   atom clusters necessary for distributing entanglement in large-scale
   systems. This technique will propel assembled-atom architectures beyond
   the threshold of quantum advantage and into a regime with abundant
   applications in quantum sensing and metrology, Rydberg-state mediated
   quantum simulation, and error-corrected quantum computation.}},
