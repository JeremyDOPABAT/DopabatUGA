Title = {{Pancharatnam-Berry optical elements for head-up and near-eye displays
   {[}Invited]}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA B-OPTICAL PHYSICS}},
Year = {{2019}},
Volume = {{36}},
