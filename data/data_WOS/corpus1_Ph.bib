@article{ ISI:000523765400001,
Author = {Zani, Louis and Bonne, Francois and Corato, Valentina and Le Coz,
   Quentin and Hoa, Christine and Lacroix, Benoit and Misiara, Nicolas and
   Nunio, Francois and Radhakrishnan, Rohan and Sedlak, Kamil and Torre,
   Alexandre and Vallcorba, Roser and Vorpahl, Christian},
Title = {{CEA Broad Studies on EU DEMO CS and PF Magnet Systems}},
