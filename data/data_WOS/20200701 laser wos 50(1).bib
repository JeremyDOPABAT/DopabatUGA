@article{ ISI:000508985000007,
Author = {Carter, John and Pathikonda, Gokul and Jiang, Naibo and Felver, Josef J.
   and Roy, Sukesh and Ranjan, Devesh},
Title = {{Time-Resolved Measurements of Turbulent Mixing in Shock-Driven
   Variable-Density Flows}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2019}},
Volume = {{9}},
Month = {{DEC 30}},
Abstract = {{Recent developments of burst-mode lasers and imaging systems have opened
   new realms of simultaneous diagnostics for velocity and density fields
   at a rate of 1 kHz-1 MHz. These enable the exploration of previously
   unimaginable shock-driven turbulent flow fields that are of significant
   importance to problems in high-energy density physics. The current work
   presents novel measurements using simultaneous measurements of velocity
   and scalar fields at 60 kHz to investigate Richtmyer-Meshkov instability
   (RMI) in a spatio-temporal approach. The evolution of scalar fields and
   the vorticity dynamics responsible for the same are shown, including the
   interaction of shock with the interface. This temporal information is
   used to validate two vorticity-deposition models commonly used for
   initiation of large scale simulations, and have been previously
   validated only via simulations or integral measures of circulation.
   Additionally, these measurements also enable tracking the evolution and
   mode merging of individual flow structures that were previously not
   possible owing to inherently random variations in the interface at the
   smallest scales. A temporal evolution of symmetric vortex merging and
   the induced mixing prevalent in these problems is presented, with
   implications for the vortex paradigms in accelerated inhomogenous flows.}},
Publisher = {{NATURE PUBLISHING GROUP}},
Address = {{MACMILLAN BUILDING, 4 CRINAN ST, LONDON N1 9XW, ENGLAND}},
Type = {{Article}},
