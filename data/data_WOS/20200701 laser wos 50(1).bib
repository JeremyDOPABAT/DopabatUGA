@article{ ISI:000505279300009,
Author = {Saniel, Monica Blaise B. and Lim, V, Len Herald and Lamorena, Rheo B.},
Title = {{An Initial Study on the Feasibility of Using Rudimentary SERS in Quick
   Chemical Assessment of Ambient Aerosols}},
Journal = {{CHEMISTRYSELECT}},
Year = {{2019}},
Volume = {{4}},
Number = {{48}},
Pages = {{14082-14090}},
Month = {{DEC 30}},
Abstract = {{A portable or field-deployable Raman spectrometer was fabricated for
   ambient particulate matter chemical assessment. The Raman spectrometer
   was optimized using five organic compounds namely, naphthalene, ascorbic
   acid, oxalic acid, sodium dodecyl sulfate (SDS), and
   polyvinylpyrrolidone (PVP), as general compositional classifiers.
   Compounds were deposited onto microscope glass slides and analyzed under
   varying laser power (30 mW - 200 mW) and integration time (3 s - 10s).
   Spectral data from the fabricated device were compared with that
   obtained from a benchtop reference device to verify correctness of
   spectral signature. Cu nanoparticles, as surface-enhanced Raman
   Spectroscopy (SERS) substrates, were applied to the samples by direct
   dropping and drop-coating. Results show that Raman intensity and signal
   to noise (S/N) ratio increases with increasing laser power and
   integration time. Results also show that the use of SERS technique
   amplified the Raman spectral signals of naphthalene and oxalic acid by
   57\% and 53.8\%, respectively, in comparison to conventional Raman.
   Notably, no additional peaks were formed from the application of
   nanoparticles and only peak intensity amplification was observed.}},
