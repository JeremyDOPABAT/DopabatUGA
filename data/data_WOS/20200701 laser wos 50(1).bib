@article{ ISI:000468600000001,
Author = {Gagnidze, Tornike and Ma, Huan and Cancellieri, Claudia and Bona,
   Gian-Luca and La Mattina, Fabio},
Title = {{Structural properties of ultrathin SrO film deposited on SrTiO3}},
